{
  "name": "@builderhub/localtunnel",
  "version": "1.0.0",
  "description": "Expose localhost to the world for BuilderHub Platform Dev",
  "main": "dist/localtunnel.js",
  "files": [
    "dist"
  ],
  "bin": {
    "lt": "dist/index.js"
  },
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "prebuild": "node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "tsc && tsc-alias",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Builderhub Platform Dev",
  "license": "MIT",
  "lint-staged": {
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@builderhub/eslint-config": "^0.0.6",
    "@builderhub/prettier-config": "^0.0.10",
    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.2.5",
    "@types/openurl": "^1.0.0",
    "@types/yargs": "^17.0.24",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "husky": "8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "13.2.2",
    "mocha": "^10.2.0",
    "pretty-quick": "3.1.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "debug": "^4.3.4",
    "module-alias": "^2.2.2",
    "openurl": "^1.1.1",
    "yargs": "^17.7.2"
  },
  "prettier": "@builderhub/prettier-config"
}
